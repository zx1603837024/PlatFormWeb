
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en" data-ng-app="app">
<head>
    <meta charset="gbk" />
    <title>一体化停车服务平台</title>
    <meta name="description" content="道路停车" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    <link rel="stylesheet" href="~/Content/css/bootstrap.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/css/animate.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/css/font-awesome.min.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/css/simple-line-icons.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/css/font.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/css/app.css" type="text/css" />
</head>
<body ng-controller="AppCtrl">
    <div class="app" id="app" ng-class="{'app-header-fixed':app.settings.headerFixed, 'app-aside-fixed':app.settings.asideFixed, 'app-aside-folded':app.settings.asideFolded, 'app-aside-dock':app.settings.asideDock, 'container':app.settings.container}" ui-view></div>


    <!-- jQuery -->
    <script src="~/Content/vendor/jquery/jquery.min.js"></script>

    <!-- Angular -->
    <script src="~/Content/vendor/angular/angular.js"></script>

    <script src="~/Content/vendor/angular/angular-animate/angular-animate.js"></script>
    <script src="~/Content/vendor/angular/angular-cookies/angular-cookies.js"></script>
    <script src="~/Content/vendor/angular/angular-resource/angular-resource.js"></script>
    <script src="~/Content/vendor/angular/angular-sanitize/angular-sanitize.js"></script>
    <script src="~/Content/vendor/angular/angular-touch/angular-touch.js"></script>
    <!-- Vendor -->
    <script src="~/Content/vendor/angular/angular-ui-router/angular-ui-router.js"></script>
    <script src="~/Content/vendor/angular/ngstorage/ngStorage.js"></script>

    <!-- bootstrap -->
    <script src="~/Content/vendor/angular/angular-bootstrap/ui-bootstrap-tpls.js"></script>
    <!-- lazyload -->
    <script src="~/Content/vendor/angular/oclazyload/ocLazyLoad.js"></script>
    <!-- translate -->
    <script src="~/Content/vendor/angular/angular-translate/angular-translate.js"></script>
    <script src="~/Content/vendor/angular/angular-translate/loader-static-files.js"></script>
    <script src="~/Content/vendor/angular/angular-translate/storage-cookie.js"></script>
    <script src="~/Content/vendor/angular/angular-translate/storage-local.js"></script>

    <!-- App -->
    <script src="~/Content/js/app.js"></script>
    <script src="~/Content/js/config.js"></script>
    <script src="~/Content/js/config.lazyload.js"></script>
    <script src="~/Content/js/config.router.js"></script>
    <script src="~/Content/js/main.js"></script>
    <script src="~/Content/js/services/ui-load.js"></script>
    <script src="~/Content/js/filters/fromNow.js"></script>
    <script src="~/Content/js/directives/setnganimate.js"></script>
    <script src="~/Content/js/directives/ui-butterbar.js"></script>
    <script src="~/Content/js/directives/ui-focus.js"></script>
    <script src="~/Content/js/directives/ui-fullscreen.js"></script>
    <script src="~/Content/js/directives/ui-jq.js"></script>
    <script src="~/Content/js/directives/ui-module.js"></script>
    <script src="~/Content/js/directives/ui-nav.js"></script>
    <script src="~/Content/js/directives/ui-scroll.js"></script>
    <script src="~/Content/js/directives/ui-shift.js"></script>
    <script src="~/Content/js/directives/ui-toggleclass.js"></script>
    <script src="~/Content/js/directives/ui-validate.js"></script>
    <script src="~/Content/js/controllers/bootstrap.js"></script>
    <!-- Lazy loading -->
</body>
</html>
