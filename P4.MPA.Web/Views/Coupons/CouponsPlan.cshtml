    @section scriptsgrid {
        // 主表
        jQuery("#grid-table").jqGrid({
            url: '../Coupons/getCouponsPlanList',
            datatype: "json",
            ajaxSelectOptions: {
                type: "GET"
            },
            mtype: "post",
            height: 'auto',
            autowidth: true,
            shrinkToFit: false,
            colNames: ["方案编号", "方案状态", "优惠券类型", "减免描述", "满减类型", "满减金额", "优惠金额", "方案创建时间", "优惠券生效时间", "优惠券失效时间",
                "发放上限(张)",
                "发放数量(张)"
            ],
            colModel: [{
                    name: 'PlanId',
                    index: 'PlanId',
                    width: 80,
                    search: false,
