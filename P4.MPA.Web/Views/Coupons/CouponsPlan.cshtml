    @section scriptsbuttonbyself {
        // 设置优惠方案
        jQuery("#grid-table").navButtonAdd("grid-pager", {
            buttonicon: "icon-plus-sign purple",
            title: "设置优惠方案",
            caption: "设置优惠方案",
            position: "top",
            onClickButton: function () {
                insertCouponsPlan();
            }
        });

        // 优惠券分发
        jQuery("#grid-table").navButtonAdd("grid-pager", {
            buttonicon: "icon-plus-sign purple",
            title: "优惠券分发",
            caption: "优惠券分发",
            position: "last",
            onClickButton: function () {
                grantCoupons();
            }
        });
    }

    // 设置优惠方案
    function insertCouponsPlan() {
        // 首次打开弹框时，当满减类型是无门槛减免或者无门槛打折时，禁用满减金额
        if ($("#Type").val() == 1 || $("#Type").val() == 2) {
            $('#TermMoney').attr("disabled", true);
        } else {
            $('#TermMoney').attr("disabled", false);
        }
        // 当满减类型是无门槛打折或者条件打折时，输入优惠折扣
        if ($("#Type").val() == 2 || $("#Type").val() == 4) {
            $('.changeDiscount1').css('display', 'none');
            $('.changeDiscount2').css('display', 'block');
        } else {
            $('.changeDiscount1').css('display', 'block');
            $('.changeDiscount2').css('display', 'none');
        }
        // 设置减免描述获取焦点和失去焦点的描述
        $("#Mark").focus(function () {
            // 输入框获取焦点
            $('.MarkDesc').css('display', 'block');
        }).blur(function () {
            // 输入框失去焦点
            $('.MarkDesc').css('display', 'none');
        });
        // 设置优惠折扣获取焦点和失去焦点的描述
        $("#CouponsMoney").focus(function () {
            // 输入框获取焦点
            $('.CouponsMoneyDesc').css('display', 'block');
        }).blur(function () {
            // 输入框失去焦点
            $('.CouponsMoneyDesc').css('display', 'none');
        });
        // 设置发放上限获取焦点和失去焦点的描述
        $("#CouponNumber").focus(function () {
            // 输入框获取焦点
            $('.CouponNumberDesc').css('display', 'block');
        }).blur(function () {
            // 输入框失去焦点
            $('.CouponNumberDesc').css('display', 'none');
        });
        layer.open({
            type: 1,
            title: ['设置优惠方案'],
            area: ['950px', '380px'],
            btn: ['提交', '取消'],
            yes: function (index, layero) {
                if (
                    $("#CouponsType").val() &&
                    $("#Mark").val() &&
                    $("#Type").val() &&
                    $("#CouponsMoney").val() &&
                    $("#StartTime").val() &&
                    $("#EndTime").val() &&
                    $("#CouponNumber").val()
                ) {
                    $.ajax({
                        type: "post",
                        url: "../Coupons/insertCouponsPlan",
                        data: {
                            "CouponsType": $("#CouponsType").val(),
                            "Mark": $("#Mark").val(),
                            "Type": $("#Type").val(),
                            "TermMoney": $("#TermMoney").val(),
                            "CouponsMoney": $("#CouponsMoney").val(),
                            "StartTime": $("#StartTime").val(),
                            "EndTime": $("#EndTime").val(),
                            "CouponNumber": $("#CouponNumber").val() == "无限" ? -1 : $(
                                "#CouponNumber").val()
                        },
                        dataType: "json",
                        success: function (res) {
                            layer.closeAll();
                            if (res.code == 200) {
                                layer.alert('优惠券方案设置完成', {
                                    icon: 1
                                }, function () {
                                    location.reload();
                                });
                            } else {
                                layer.alert(res.msg, {
                                    icon: 2
                                }, function () {
                                    layer.closeAll();
                                });
                            }
                        }
                    });
                } else {
                    layer.alert('请输入信息', {
                        icon: 2
                    });
                }
            },
            btn2: function (index, layero) {
                layer.closeAll();
            },
            // 层销毁后触发的回调：无论是确认还是取消，只要层被销毁了，end都会执行，不携带任何参数。
            end: function () {
                // 当满减类型是无门槛减免或者无门槛打折时，禁用满减金额
                if ($("#Type").val() == 1 || $("#Type").val() == 2) {
                    $('#TermMoney').attr("disabled", true);
                } else {
                    $('#TermMoney').attr("disabled", false);
                }
                // 当满减类型是无门槛打折或者条件打折时，输入优惠折扣
                if ($("#Type").val() == 2 || $("#Type").val() == 4) {
                    $('.changeDiscount1').css('display', 'none');
                    $('.changeDiscount2').css('display', 'block');
                } else {
                    $('.changeDiscount1').css('display', 'block');
                    $('.changeDiscount2').css('display', 'none');
                }
            },
            shadeClose: true,
            content: $("#lay_postinfo")
        });
    }

    // 优惠券分发
    function grantCoupons() {
        let PlanId = jQuery("#grid-table").jqGrid('getGridParam', 'selrow');
        console.log(PlanId, 'PlanId');
        if (!PlanId) {
            swal({
                title: "请选择优惠方案！",
                timer: 1000,
                type: "error",
                showConfirmButton: false
            });
            return;
        }
        layer.open({
            type: 1,
            title: ['优惠券发放'],
            area: ['1000px', '500px'],
            btn: ['提交', '取消'],
            yes: function (index, layero) {
                let rowIds = jQuery("#grid-table1").jqGrid('getGridParam', 'selarrrow');
                let selectRowData = [];
                for (let k = 0; k < rowIds.length; k++) {
                    let rowData = $("#grid-table1").jqGrid('getRowData', rowIds[k]);
                    selectRowData.push(rowData);
                }
                console.log(selectRowData, 'selectRowData');
                let uid = []
                selectRowData.map((item, i) => {
                    if (item.uid) {
                        uid.push(item.uid)
                    }
                })
                uid = uid.join(',')
                console.log(uid, 'uid');
                if (rowIds.length != 0) {
                    $.ajax({
                        type: "post",
                        url: "../Coupons/GrantCoupons",
                        data: {
                            "uid": uid,
                            "PlanId": PlanId
                        },
                        dataType: "json",
                        success: function (res) {
                            layer.closeAll();
                            if (res.code == 200) {
                                layer.alert('优惠券分发成功', {
                                    icon: 1
                                }, function () {
                                    location.reload();
                                });
                            } else {
                                layer.alert(res.msg, {
                                    icon: 2
                                }, function () {
                                    layer.closeAll();
                                });
                            }
                        }
                    });
                } else {
                    layer.alert('请勾选需要分发的用户', {
                        icon: 2
                    });
                }
            },
            btn2: function (index, layero) {
                layer.closeAll();
            },
            shadeClose: true,
            content: $("#grantCoupons")
        });
    }

    // 满减类型
    function Type(cellvalue, options) {
        switch (cellvalue) {
            case 1:
                return "无门槛减免";
                break;
            case 2:
                return "无门槛打折";
                break;
            case 3:
                return "条件减免";
                break;
            case 4:
                return "条件打折";
                break;
            default:
                return "";
        }
    }

    // 满减金额
    function TermMoney(cellvalue, options) {
        switch (cellvalue) {
            case null:
                return "";
                break;
            default:
                return cellvalue;
        }
    }

    // 发放上限
    function CouponNumber(cellvalue, options) {
        switch (cellvalue) {
            case -1:
                return "无限";
                break;
            default:
                return cellvalue;
        }
    }

    // 优惠券类型
    function CouponsType(cellvalue, options) {
        switch (cellvalue) {
            case 1:
                return "月卡套餐";
                break;
            case 2:
                return "钱包充值";
                break;
            case 3:
                return "道路停车";
                break;
            case 4:
                return "停车场停车";
                break;
            default:
                return "";
        }
    }

    // 方案状态
    function Status(cellvalue, options) {
        switch (cellvalue) {
            case 1:
                return "<span class='badge badge-success'>启用</span>";
                break;
            case 2:
                return "<span class='badge badge-danger'>停用</span>";
                break;
            case 3:
                return "<span class='badge badge-warning'>过期</span>";
                break;
            case 4:
                return "<span class='badge badge-secondary'>终止</span>";
                break;
            default:
                return "";
        }
    }
</script>