                    formatoptions: {
                        srcformat: 'Y-m-d H:i:s',
                        newformat: 'Y-m-d H:i:s'
                    },
                }
            ],
            viewrecords: true,
            rowNum: 10,
            rowList: [10, 20, 30],
            pager: "#grid-pager1",
            altRows: true,
            sortname: 'PlanId',
            sortorder: "asc",
            toppager: false,
            editurl: "../Coupons/CouponsEditDelete",
