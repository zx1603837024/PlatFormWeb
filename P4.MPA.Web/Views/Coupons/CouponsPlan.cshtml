            }
        });

        // 用户信息表
        jQuery("#grid-table1").jqGrid({
            url: '../VideoConfig/getTUser',
            datatype: "json",
            ajaxSelectOptions: {
                type: "GET"
            },
            mtype: "post",
            shrinkToFit: false,
            colNames: ['用户名', 'uid', '手机号', '车牌一', '车牌二', '车牌三', '钱包', '创建时间'],
            colModel: [{
                    name: 'nickName',
                    index: 'nickName',
                    width: 120,
                },
                {
                    name: 'uid',
                    index: 'uid',
                    width: 270,
                },
                {
                    name: 'tel',
                    index: 'tel',
                    width: 120,
                },
                {
                    name: 'CarNumber1',
                    index: 'CarNumber1',
                    width: 100,
                },
                {
                    name: 'CarNumber2',
                    index: 'CarNumber2',
                    width: 100,
                },
