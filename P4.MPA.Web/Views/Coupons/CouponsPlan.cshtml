                            2: '停用',
                        },
                        sopt: ['eq']
                    }
                },
                {
                    name: 'CouponsType',
                    index: 'CouponsType',
                    formatter: CouponsType,
                    searchoptions: {
                        value: {
                            1: '月卡套餐',
                            2: '钱包充值',
                            3: '道路停车',
                            4: '停车场停车',
                        },
                        sopt: ['eq']
                    }
                },
                {
                    name: 'Mark',
                    index: 'Mark',
                    search: false,
                    editable: true,
                    key: true,
                },
                {
                    name: 'Type',
