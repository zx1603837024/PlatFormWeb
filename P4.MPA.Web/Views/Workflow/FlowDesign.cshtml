        function callbackSuperDialog(selectValue) {
            var aResult = selectValue.split('leipi');
            $('#' + window._viewField).val(aResult[0]);
            $('#' + window._hidField).val(aResult[1]);
            //document.getElementById(window._hidField).value = aResult[1];

